<header>
	<div class="navbar navbar-fixed-top">
		<div class="navbar-inner">
			<%= link_to "McCown Archaeobotany Laboratary", root_path, id:"logo" %>
			<div class="container-fluid">
				<nav>
					<ul class="nav pull-right">
						<li><%= link_to "Home", root_path %></li>
						<li><%= link_to "Research", research_path %></li>
						<li><%= link_to "People", people_path %></li>
						<li><%= link_to "Resource", resource_path %></li>
						<li><%= link_to "Contact", contact_path %></li>
					</ul>
				</nav>
			</div>
		</div>
	</div>

	<div class="center container-fluid hero-unit">
		<div class="row-fluid">
			<div class="span3">
				<!--
				<ul class="unstyled pull-left">
					<li><%= link_to "Laboratary Reports", '#' %></li>
					<li><%= link_to "Contact Information", '#' %></li>
					<li><%= link_to "Haikus", '#' %></li>
				</ul>
				-->
			</div>
			<div class="span9">

				<div id="myCarousel" class="carousel slide">
  					<ol class="carousel-indicators">
   						<li data-target="#myCarousel" data-slide-to="0" class="active"></li>
    					<li data-target="#myCarousel" data-slide-to="1"></li>
    					<li data-target="#myCarousel" data-slide-to="2"></li>
  					</ol>
  					<!-- Carousel items -->
  					<div class="carousel-inner">
    					<div class="active item">
    						<%= image_tag "flotation.jpg", alt:"Floatation in the farm" %>
    					</div>
    					<div class="item">
    						<%= image_tag "group_lab.JPG", alt:"Group picture in the lab" %>
    					</div>
   	 					<div class="item">
   	 						<%= image_tag "group_out.JPG", alt:"Group picture on campus" %>
   	 					</div>
  					</div>
